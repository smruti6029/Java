	public static boolean vallidUser(int user_id) 
	
	{
		List<UsersInputClass> users=UserQuestionAnswerById.getQuestionanswer();
		for(UsersInputClass user:users)
		{
			if(user.getId()==user_id)
			{
				return 
			}
		}
		return false;
	}

}
